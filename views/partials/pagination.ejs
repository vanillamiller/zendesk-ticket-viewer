<div id="pagination-wrap">
    <nav aria-label="Page navigation">
        <ul class="pagination">
            <% if(page != 1) { %>
            <li class="page-item"><a class="page-link" href="/tickets?page=1">First</a></li>

            <% } else {%>
            <li class="page-item disabled"><a class="page-link">First</a></li>

            <% } %>

            <% for (i=1; i <= pages ; i++) { %>
            <%if (i == page) { %>
            <li class="page-item active"><a class="page-link" href="/tickets?page=<%=i%>">
                    <%= i %></a></li>
            <% } else { %>
            <li class="page-item"><a class="page-link" href="/tickets?page=<%=i%>">
                    <%= i %></a></li>
            <% } %>
            <% } %>
            <% if(page != pages) { %>

            <li class="page-item"><a class="page-link" href="/tickets?page=<%=pages%>">Last</a></li>
            <% } else {%>
            <li class="page-item disabled"><a class="page-link">Last</a></li>
            <% } %>

        </ul>
    </nav>
</div>